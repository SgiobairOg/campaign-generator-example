{
    "name": "campaign-generator-example",
    "version": "1.1.0",
    "description": "Building static, multi-language, campaign sites, using node and webpack",
    "main": "main.js",
    "scripts": {
        "start": "node main.js",
        "check": "c8 --check-coverage --lines 100 npm test",
        "test": "mocha 'src/**/*.test.js'",
        "build": "node './src/scripts/build-site.js'",
        "serve": "live-server --port=5555 --verbose"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sgiobairog/campaign-generator-example.git"
    },
    "keywords": [
        "webpack",
        "nodejs",
        "multi-lang"
    ],
    "author": "Jason Wilson <jason@wilsons.io>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/sgiobairog/campaign-generator-example/issues"
    },
    "homepage": "https://github.com/sgiobairog/campaign-generator-example#readme",
    "devDependencies": {
        "aws-sdk": "^2.393.0",
        "c8": "^3.3.0",
        "chai": "^4.2.0",
        "eslint-config-prettier": "^3.6.0",
        "live-server": "^1.2.1",
        "minify": "^4.1.0",
        "mocha": "^5.2.0",
        "path": "^0.12.7",
        "webpack": "^4.29.0"
    },
    "dependencies": {
        "args": "^5.0.0",
        "fs-extra": "^7.0.1",
        "i18n": "^0.8.3"
    }
